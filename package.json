    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.13",
    "@types/vscode": "^1.98.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.3.0",
    "typescript": "^5.8.2",
    "@types/react": "^19.0.12",
