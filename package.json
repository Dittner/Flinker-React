  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dittner/Flinker-React.git"
  },
  "keywords": [
    "RXObserver.ts",
    "React",
    "App state manager"
  ],
  "author": "Alexander Dittner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dittner/Flinker/issues"
  },
  "homepage": "https://github.com/Dittner/Flinker-React",
  "private": false,
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
